{
  "name": "standard-express-api",
  "version": "1.0.0",
  "description": "Easily create an express server with basic configurations",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "build": "tsc && npm run lint && npm run test",
    "link": "npm run build && npm link",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardocrescenti/standard-express-api.git"
  },
  "keywords": [
    "express",
    "standard",
    "api",
    "graphql"
  ],
  "author": {
    "name": "Ricardo Crescenti",
    "email": "ricardo.crescenti@gmail.com",
    "url": "https://github.com/ricardocrescenti"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ricardocrescenti/standard-express-api/issues"
  },
  "homepage": "https://github.com/ricardocrescenti/standard-express-api#readme",
  "dependencies": {
    "@types/express": "^4.17.4",
    "@types/knex": "^0.16.1",
    "apollo-server-express": "^2.12.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "graphql-schema-creator": "git+https://github.com/ricardocrescenti/graphql-schema-creator.git#master",
    "knex": "^0.20.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "jest": "^25.2.3",
    "pg": "^8.0.0",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
